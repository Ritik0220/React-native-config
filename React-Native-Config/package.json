{
  "name": "horizon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build-staging":"cp ./.env.staging ./.env && npx react-native run-android",
    "build-production":"cp ./.env.production ./.env && npx react-native run-android",
    "release-staging":"cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "release-production":"cd android && ENVFILE=.env.production ./gradlew assembleRelease"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
